{
  "manifest_version": 2,
  "name": "Washington Post Cleaner",
  "description": "Removes extra junk from Washington Post article pages",
  "version": "0.61",
  "content_scripts": [
    {
      "matches": ["*://www.washingtonpost.com/*/*"],
      "exclude_matches": ["*://www.washingtonpost.com/regional/"],
      "css": ["mystyles.css"]
    }
  ]
}
